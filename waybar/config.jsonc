{
  "output": "DP-1",
  "layer": "top",
  "spacing": 8,
  "position": "bottom",
  "height": 32,
  "gtk-layer-shell": true,
  "modules-left": [
    "hyprland/workspaces",
    "cava",
    "custom/media"
  ],
  "modules-center": [
    "wlr/taskbar"
  ],
  "modules-right": [
    "keyboard-state",
    "tray",
    "idle_inhibitor",
    "custom/weather",
    "battery",
    "battery#charging",
    "gamemode",
    "pulseaudio",
    "network",
    "custom/dunst",
    "clock"
  ],
  "hyprland/workspaces": {
    "all-outputs": false,
    "format": "{icon}",
    "format-icons": {
      "active": "ÔÜí",
      "default": "ÔÑë"
    },
    "sort-by": "ID",
    "persistent-workspaces": {
      "1": [
        "DP-1"
      ],
      "2": [
        "DP-1"
      ],
      "3": [
        "DP-1"
      ],
      "4": [
        "DP-1"
      ]
    }
  },
  "keyboard-state": {
    "numlock": false,
    "capslock": true,
    "format": "{icon}",
    "format-icons": {
      "locked": "<span font=\"Material Design Icons\">ÔÄ£</span>",
      "unlocked": ""
    }
  },
  "cava": {
    "framerate": 165,
    "autosens": 1,
    // "sensitivity": 100,
    "bars": 14,
    "lower_cutoff_freq": 50,
    "higher_cutoff_freq": 10000,
    "method": "pipewire",
    "source": "auto",
    "stereo": true,
    "reverse": false,
    "bar_delimiter": 0,
    "monstercat": false,
    "waves": false,
    "noise_reduction": 0.77,
    "input_delay": 2,
    "format-icons": [
      "‚ñÅ",
      "‚ñÇ",
      "‚ñÉ",
      "‚ñÑ",
      "‚ñÖ",
      "‚ñÜ",
      "‚ñá",
      "‚ñà"
    ],
    "actions": {
      "on-click-right": "mode"
    }
  },
  "idle_inhibitor": {
    "format": "{icon}",
    "format-icons": {
      "activated": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞Ö∂</span>",
      "deactivated": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞æ´</span>"
    }
  },
  "tray": {
    "spacing": 8,
    "icon-size": 16,
    // "show-passive-items": true
  },
  "clock": {
    "format": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ôπ</span> {:%A, %b %d %I:%M %p}",
    "on-click": "xdg-open https://calendar.google.com",
    "tooltip-format": "<tt><small>{calendar}</small></tt>",
    "calendar": {
      "mode": "month",
      "mode-mon-col": 3,
      "on-scroll": 1,
      "on-click-right": "mode",
      "format": {
        "months": "<span color='#ffead3'><b>{}</b></span>",
        "days": "<span color='#ecc6d9'><b>{}</b></span>",
        "weeks": "<span color='#99ffdd'><b>W{}</b></span>",
        "weekdays": "<span color='#ffcc66'><b>{}</b></span>",
        "today": "<span color='#ff6699'><b><u>{}</u></b></span>"
      }
    },
    "actions": {
      "on-click-right": "mode",
      "on-click-forward": "tz_up",
      "on-click-backward": "tz_down",
      "on-scroll-up": "shift_up",
      "on-scroll-down": "shift_down"
    }
  },
  "battery": {
    "bat": "hidpp_battery_0",
    "states": {
      "full": 100,
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞çΩ</span>{capacity}%",
    "format-charging": "",
    "format-plugged": "",
    "format-alt": "{time} {icon}",
    "format-good": "",
    "format-full": "",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ]
  },
  "battery#charging": {
    "bat": "hidpp_battery_1",
    "states": {
      "full": 100,
      "good": 95,
      "warning": 30,
      "critical": 15
    },
    "format": "",
    "format-charging": "Ôóß {capacity}%",
    "format-plugged": "Ôá¶ {capacity}%",
    "format-alt": "{time} {icon}",
    "format-good": "",
    "format-full": "",
    "format-icons": [
      "ÔâÑ",
      "ÔâÉ",
      "ÔâÇ",
      "ÔâÅ",
      "ÔâÄ"
    ]
  },
  "network": {
    "format-wifi": "Ôá´",
    "format-ethernet": "<span font=\"Material Design Icons\" size='large' font_weight='normal'>Û∞àÄ</span>",
    "tooltip-format": "{gwaddr} via {ifname}",
    "format-linked": "{ifname} (No IP) Û∞àÄ",
    "format-disconnected": "Disconnected ‚ö†",
    "format-alt": "Û∞àÄ {ipaddr}/{cidr}"
  },
  "pulseaudio": {
    "format": "{icon} {format_source}",
    "format-muted": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ùü</span> {format_source}",
    "format-source": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ç¨</span>",
    "format-source-muted": "Û∞ç≠",
    "format-icons": {
      "headphone": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ãã</span>",
      "speaker": [
        "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ïø</span>",
        "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ñÄ</span>",
        "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ïæ</span>"
      ]
    },
    "on-click": "pavucontrol",
    "on-click-right": "helvum"
  },
  "custom/media": {
    "format": "{icon}   {}",
    "return-type": "json",
    "max-length": 40,
    "format-icons": {
      "spotify": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ìá</span>",
      "default": "üéú"
    },
    "escape": true,
    "exec": "$HOME/.dotfiles/scripts/mediaplayer.py --player spotify 2> /dev/null"
  },
  "custom/weather": {
    "format": "{}",
    "format-alt": "{alt}: {}",
    "format-alt-click": "click-right",
    "interval": 1800,
    "return-type": "json",
    "exec": "$HOME/.dotfiles/scripts/weather.sh Columbus",
    "exec-if": "ping wttr.in -c1"
  },
  "custom/dunst": {
    "exec": "~/.dotfiles/scripts/dunst_tray.sh",
    "on-click": "dunstctl set-paused toggle",
    "restart-interval": 1
  },
  "wlr/taskbar": {
    "format": "{icon}",
    "all-outputs": true,
    "icon-size": 24,
    "icon-theme": "Mint-L-Orange",
    "tooltip-format": "{title}",
    "on-click": "activate",
    "on-click-middle": "close",
    "app_ids-mapping": {
      "firefoxdeveloperedition": "firefox-developer-edition"
    },
    "rewrite": {
      "Firefox Web Browser": "Firefox",
      "Foot Server": "Terminal"
    }
  },
  "gamemode": {
    "format": "{glyph}",
    "format-alt": "{glyph} {count}",
    "glyph": "<span font=\"Material Design Icons\" size='large' font_weight='normal' rise='-1500'>Û∞ä¥</span>",
    "hide-not-running": true,
    "use-icon": true,
    "icon-name": "input-gaming-symbolic",
    "icon-spacing": 4,
    "icon-size": 20,
    "tooltip": true,
    "tooltip-format": "Games running: {count}"
  }
}
